{
  "expo": {
    "name": "V-Stock Delivery",
    "slug": "v-stock-delivery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vstock.app",
      "buildNumber": "1.0.1"
    },
    "android": {
      "package": "com.vstock.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/RBL-Icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/RBL-Icon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notiIcon.png"
        }
      ]
    ]
  }
}